@model DepartmentDetailsDto
@using StudentEnrollment.Core.Dtos;


@{
    ViewData["Title"] = "Departments";
}
            <div class="alert alert-danger">Are you sure you want to delete this Department?
    </div>
       



        <form asp-action="Delete" asp-route-id="@Model.Id" method="post" role="form">


                
